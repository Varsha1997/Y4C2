@model Y4C2.Models.AddContent

@{
    ViewData["Title"] = "PlayVideo";
}


<title>Y4C</title>
<body>
    <br />
    <br />
    <center>
        <h1>@Model.Title</h1>
        <h4>@Model.Description</h4>
    </center>
    <hr />
    <br />
    <center>
        <iframe width="560"
                height="315"
                src="@Model.Video"
                frameborder="0"
                allowfullscreen></iframe><br /><br />

        <iframe width="560"
                height="100"
                src="@Model.Audio"
                frameborder="0"
                allowfullscreen></iframe>
        <br />

        <hr />
        <div class="blog">
            @Html.Raw(Model.BlogContent)
        </div>

    </center>
</body>
