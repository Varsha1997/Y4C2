
@model System.Collections.Generic.IEnumerable<Y4C2.Models.Questions>
@{
    ViewData["Title"] = "ViewSurvey";
}
<head>
    <link rel="stylesheet" type="text/css" href="~/css/SideNav.css" />
    <link rel="stylesheet" type="text/css" href="~/css/AdminHome.css" />
    <!--<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>-->
    <link rel="stylesheet" type="text/css" href="~/css/Range.css" />
    <script type="text/javascript" src="~/js/Range.js"></script>
    <link rel="stylesheet" type="text/css" href="~/lib/bootstrap/dist/css/bootstrap.css" />

</head>
<h2>AdminHome</h2>
<body>
    <center>
        <h3>
            <strong>Please complete the survey below to continue.</strong>
        </h3>
        <!--
        <p style="font-size:42px;">@ViewBag.Message</p>
        -->
        <label>
            1.  How did you feel before completing this theme? <br /><br />
            <div class="slidecontainer">
                 <input type="range" min="1" max="10"  class="slider" id="myRange" >
            </div>
        </label><br /><br />
        <label>
            2.  How did you feel after completing this theme? <br /><br />
            <div class="slidecontainer">
                 <input type="range" min="1" max="10"  class="slider" id="myRangeTwo" >
            </div>
        </label><br /><br />
        <label>
            3. What did you enjoy most about this theme? <br />
            <textarea rows="5" cols="50" ></textarea>
        </label>

        <div style="padding-top:2em;">
            <input type="button" value="Continue" class="btn btn-default" onclick="location.href='@Url.Action("StudentDashboard", "Account")'" />
        </div>
    </center>

    <!--Note correct complete survey saved in notepadd++-->

</body>
